<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <RootNamespace>Warehouser_WPF</RootNamespace>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="WindowsAPICodePack-Shell" Version="1.1.1" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Hi_Settings.Designer.cs">
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
      <AutoGen>True</AutoGen>
      <DependentUpon>Hi_Settings.settings</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <None Update="Dictionary1r.settings">
      <Generator>SettingsSingleFileGenerator</Generator>
    </None>
    <None Update="Hi_Settings.settings">
      <Generator>SettingsSingleFileGenerator</Generator>
      <LastGenOutput>Hi_Settings.Designer.cs</LastGenOutput>
    </None>
  </ItemGroup>

</Project>
